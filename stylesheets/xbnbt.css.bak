/* ----- xbnbt.css by =Xotic= for use with XBNBT 81b.3.5  ---- */

/* ----- general ----- */
	/* ----- standard body elements ----- */
/*		body{background-color: #1E90FF}*/
		body{background-color: #ccccff}
		body{font-family:tahoma}
		body{font-size:12pt}
		body{color:black}
		body{font-weight:normal}
	/* ----- standard heading elements ----- */
		h1{font-size:20pt; color: Black}
		h1{font-weight:normal}
		h1{letter-spacing: 0.025cm}
		h1{text-align:center}

		h2{font-size:15pt; color: Black}
		h2{font-weight:normal}
		h2{letter-spacing: 0.025cm}
		h2{text-align:center}

		h3{font-size:15pt; color: Black}
		h3{font-weight:normal}
		h3{letter-spacing: 0.025cm}
		h3{text-align:center}
	/* ----- standard text elements ----- */
		p{font-family:tahoma}
		p{font-size:10pt}
		p{color:black}
		p{font-weight:bold}
	/* ----- standard hypertext elements ----- */
	/*	a{text-decoration:none}		*/
		a{text-decoration:underline}
		a{font-size:10pt}
		a{font-weight:bold}
		a{color:black}
		a:hover{text-decoration:underline}
		a:hover{color:red}
	/*	a:hover{color:#404040}		*/
	/* ----- standard input elements ----- */
		input{font-family:tahoma}
		input{font-size:10pt}
		input{overflow:hidden}
		textarea{font-family:tahoma}
		textarea{font-size:10pt}
	/* ----- standard image elements ----- */
		img{border-width:0px} 
	/* ----- standard table elements ----- */
		table{margin-left:auto; margin-right:auto}
		table{border-width:2pt}
		table{border-collapse:collapse}
	/*	table{border:2px solid #8080ff } /* ----- optional border around entire table ----- */
	/* ----- standard table rows ----- */
		tr{}
	/* ----- standard table headers ----- */
		th{background-color:#8080ff}
		th{white-space:nowrap}
		th{font-family:tahoma}
		th{font-size:9pt}
		th{font-weight:bold}
		th{color:black}
		th{padding: 5px 10px 5px 10px}
		th{border:1.0px solid black}
		th{vertical-align:top}
	/* ----- standard table data ----- */
		td{font-size:9pt}
		td{padding: 5px 10px 5px 10px}
		td{white-space:nowrap}	
		td{border:1.0px solid black}

		code{font-family:tahoma}
		code{font-size:9pt}
	/* ----- specific text elements ----- */
		p.delete{text-align:center}
		p.deleted{text-align:center}
		p.search_results{text-align:center}
		p.clearfilter{display:none}
		p.search_filter{text-align:center}
		p.results{font-family:tahoma; font-size:10pt; text-align:center}
		p.denied{text-align:center}
		p.announce{}
		p.js_warning{text-align:center; font-size:medium}
		p.redirect{text-align:center}
